<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="globalgetnet" type="host">*.globalgetnet.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="globalgetnet" type="host">*.globalgetnet.com</value>
                <value id="iesnare" type="host">*.iesnare.com</value>
                <value id="iesnare-wss" type="host">wss://mpsnare.iesnare.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="globalgetnet" type="host">*.globalgetnet.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="globalgetnet" type="host">*.globalgetnet.com</value>
            </values>
        </policy>
        <policy id="default-src">
            <values>
                <value id="globalgetnet" type="host">*.globalgetnet.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
